{
	"id": "nodebb-plugin-sso-cloudtrust",
	"name": "NodeBB OAuth SSO",
	"description": "NodeBB Plugin that allows users to login/register via any configured OAuth provider.",
	"url": "https://github.com/EdisonSu768/nodebb-plugin-sso-oauth",
	"library": "./library.js",
	"scripts": [ "sso.js" ],
	"hooks": [
		{ "hook": "action:auth.overrideLogin", "method": "login" },
		{ "hook": "static:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:auth.init", "method": "getStrategy" }
	]
}
